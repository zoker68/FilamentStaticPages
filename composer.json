{
    "name": "zoker/filament-static-pages",
    "description": "Simple plugin for static pages",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Zoker\\FilamentStaticPages\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Maksim Martynov",
            "email": "zokermm@gmail.com",
            "homepage": "https://github.com/zoker68"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.1",
        "spatie/laravel-package-tools": "^1.16",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "larastan/larastan": "^v3.8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zoker\\FilamentStaticPages\\FilamentStaticPagesServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "analyze": "php -d memory_limit=1G vendor/bin/phpstan analyse"
    }
}
